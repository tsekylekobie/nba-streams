{
	"name": "NBA Streams",
	"description" : "Find NBA streams quickly. Streamers can be found on r/nbastreams.",
	"version": "1.0",
	"permissions": [
    	"activeTab"
    ],
    "background": {
    	"scripts": ["js/main.js", "js/reddit.min.js", "js/jquery-3.3.1.min.js"],
    	"persistent": false
    },
	"browser_action": {
      "default_popup": "index.html",
      "default_icon": "basketball.png"
    },
    "manifest_version": 2
}